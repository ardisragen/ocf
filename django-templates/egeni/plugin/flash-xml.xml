{% load ch_extras %}<?xml version="1.0" encoding="UTF-8"?><document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">{% for nodeId, node_info in nodes_dict.items %}<node><id>{{ nodeId }}</id><x>{{ node_info.x }}</x><y>{{ node_info.y }}</y><img>{{ node_info.node.img_url }}</img><name>{{ nodeId }}</name><is_selected>{{ node_info.node|is_in_set:slice.nodes.all }}</is_selected><has_err>False</has_err></node>{% endfor %}{% for link in links %}<link><id>{{ link.id }}</id><src>{{ link.src.ownerNode.nodeId }}</src><dst>{{ link.dst.ownerNode.nodeId }}</dst><is_selected>{{ link|is_in_set:slice.links.all }}</is_selected><has_err>False</has_err></link>{% endfor %}</document>