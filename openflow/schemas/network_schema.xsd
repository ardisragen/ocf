xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="/opt/foamofelia/ofelia/foam/schemas/" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="datapath">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="port" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute type="xs:int" name="num" use="optional"/>
                <xs:attribute type="xs:string" name="name" use="optional"/>
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute type="xs:string" name="component_id" use="optional"/>
      <xs:attribute type="xs:string" name="component_manager_id" use="optional"/>
      <xs:attribute type="xs:string" name="dpid" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="link">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute type="xs:string" name="srcDPID" use="optional"/>
          <xs:attribute type="xs:byte" name="srcPort" use="optional"/>
          <xs:attribute type="xs:string" name="dstDPID" use="optional"/>
          <xs:attribute type="xs:byte" name="dstPort" use="optional"/>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
</xs:schema>
