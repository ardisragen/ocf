#inform user
printHeader "info#" "Using default post-upgrade-hook module..."

#Starting apache
print "Starting apache2..."
/usr/sbin/service apache2 start

if [ $OFELIA == 1 ];then
	print "Setting default PolicyEngine rule/s..."
	#Setting default PolicyEngineRules
	python $SRC_DIR/bin/versions/0.3/upgrade/tools/DefaultPolicyEngineRules.py
fi
